  "scripts": {
    "dev": "webpack --mode=development --env.NODE_ENV=development --progress --hide-modules --hot --watch",
    "test": "webpack --mode=development --env.NODE_ENV=test --progress --hide-modules --hot --watch",
    "prod": "webpack --mode=development --env.NODE_ENV=k8s-b --progress --hide-modules --hot --watch",
    "lint": "eslint --ext .js,.vue --fix ./src",
    "buildProd": "npm run packProd && npm run remove-evals && npm run zipProd",
    "buildProdK8s-B": "npm run packProdK8s-B && npm run remove-evals && npm run zipProdK8s-B",
    "buildProdK8s-huahuo": "npm run packProdK8s-huohua && npm run remove-evals && npm run zipProdK8s-huohua",
    "buildProdK8s-meishubao": "npm run packProdK8s-meishubao && npm run remove-evals && npm run zipProdK8s-meishubao",
    "buildTest": "npm run packTest && npm run remove-evals && npm run zipTest",
    "packProd": "webpack --mode=production --env.NODE_ENV=production --progress --hide-modules",
    "packProdK8s-B": "webpack --mode=production --env.NODE_ENV=k8s-b --progress --hide-modules",
    "packProdK8s-huohua": "webpack --mode=production --env.NODE_ENV=k8s-huohua --progress --hide-modules",
    "packProdK8s-meishubao": "webpack --mode=production --env.NODE_ENV=k8s-meishubao --progress --hide-modules",
    "packTest": "webpack --mode=production --env.NODE_ENV=test --progress --hide-modules",
    "remove-evals": "node scripts/remove-evals.js",
    "zipProd": "cross-env NODE_ENV=production node scripts/build-zip.js",
    "zipProdK8s-B": "cross-env NODE_ENV=production PACK_ENV=b node scripts/build-zip.js",
    "zipProdK8s-huohua": "cross-env NODE_ENV=production PACK_ENV=huohua node scripts/build-zip.js",
    "zipProdK8s-meishubao": "cross-env NODE_ENV=production PACK_ENV=meishubao node scripts/build-zip.js",
    "zipTest": "cross-env NODE_ENV=test node scripts/build-zip.js"
  },
  "version": "1.0.0"
}
